<article id="J_article" class="J-article article">
    <div id="title">
        <div class="article-title">
            <h1 class="title">$!{page.title}</h1>
        </div>
        <div class="article-src-time">
            <span class="src">$!{page.pageSource}&nbsp;&nbsp;&nbsp;&nbsp;$!{time}</span>
        </div>
    </div>
    <div id="content" class="J-article-content article-content">
        #set($idx=0)
        #foreach($zw in $!{zws})
        #if($zw == "" || $zw == "null")
        #elseif($zw.contains("$#imgidx"))
        #set($img = $!{page.imgs.get($idx).src})
        #set($gifimg = $!gifimglist[$idx])
        #set($paddingValue=$!{page.imgs.get($idx).imgheight}*1.0/$!{page.imgs.get($idx).imgwidth}*100)
        <figure class="section img">
            <a class="img-wrap" style="padding-bottom:$paddingValue%;" href="$!{img}" data-size="$!{page.imgs.get($idx).imgwidth} x $!{page.imgs.get($idx).imgheight}">
                <img width="100%" alt="$!{page.title}" src="$!{img}" data-weight="588" data-width="$!{page.imgs.get($idx).imgwidth}" data-height="$!{page.imgs.get($idx).imgheight}" gif-minisrc="$!{gifimg}">
            </a>
        </figure>
        #set($idx=$idx+1)
        #else
        <p class="section txt">$zw</p>
        #end
        #end
    </div>
    <p class="article-tag clearfix">
        #foreach($param in ${tagMap.entrySet()})
        <a href="http://gxjifen.dftoutiao.com/gx-ued-jser/wangzhijun/msports.resources/msports.east.webpack/build/html/query-tag.html?typecode=${param.key}&name=${param.value}">${param.value} </a>
        #end
    </p>
</article>

